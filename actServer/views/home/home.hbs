  <script>
    new MultiSelectTag('technologies')  // id
  </script>
  <script>
    const currentYear = new Date().getFullYear();
const select = document.getElementById("currYear");

for (let year = currentYear; year >= currentYear - 4; year--) {
  const option = document.createElement("option");
  option.value = year;
  option.text = year;
  select.appendChild(option);
}
  </script>
<script>
    new MultiSelectTag('technologies')  // id
  </script>
  <script>
    const text = "Hello, Welcome to DevSpace: \n A project management website";

    let i = 0;
    function typingEffect() {
      if (i < text.length) {
        document.getElementById("typing").innerHTML += text.charAt(i);
        i++;
        setTimeout(typingEffect, 60);
      }
    }

    typingEffect();

  </script>
