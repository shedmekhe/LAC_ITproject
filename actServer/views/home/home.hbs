  <section class="homepage" id="menu">
    <div class="projects-div">
      <div class="project-content">
        <h1>Search projects of IT department in few clicks....</h1>
      </div>
      <div class="dropdown">
        <div class="drop-box">
          <form action="/home" method="POST">
            <div class="dropdowns">
              <label for="year">Year:</label><br>
              <select name="year" id="currYear"><option value="none" selected disabled hidden>Select year</option></select>
            </div>
            <div class="dropdowns" class="select">
              <label for="academic">Academic year</label><br>
              <select id="academic" name="academic">
                <option value="none" selected disabled hidden>Select an Option</option>
                <option value="SY">Second Year</option>
                <option value="TY">Third Year</option>
                <option value="BTECH">B.TECH</option>
              </select>
            </div>
            <div class="dropdowns" class="select">
              <label for="semester">Semester</label><br>
              <select id="semester" name="semester">
                <option value="none" selected disabled hidden>Select an Option</option>
                <option value="SEM-I">Semester I</option>
                <option value="SEM-II">Semester II</option>
              </select>
            </div>
            <div class="search-button">
            <button type="submit" id="search-button">Search</button>
            </div>
          </form>
        </div>
      </div>
      {{!-- <div id='myFooter'>
        <a href='#myFooter' />
      </div> --}}
      {{!-- hello="../../" --}}
      <div class="container">
        <div class="border-div">
          <div class="cards grid-row">
            {{#proj}}
            <div class="card">
              <div class="card-top">
                <a href="/project/{{title}}">
                  <img src="{{file2}}" width="200" height="100">
              </div>
              <div class="card-info">
                <h2>{{title}}</h2>
                <span class="date">{{currentDate}}</span>
                <h4>
                  <p class="details">{{obj}}</p>
                </h4>
                <h2 class="headlines pd-1" style="margin-bottom: 30px;margin-top: 25px;">{{technologies}}</h2>

                {{!-- <span>{{technologies}}</span> --}}
              </div>
              <div class="card-bottom flex-row">
                <!-- <a href="#" class="read-more">View Project</a> -->
                {{!-- <a href="#" class="button btn-sky">View Project</a> --}}
              </div>
              </a>
            </div>
            {{/proj}}
          </div>
          <div class="pagination">
          <li class="page-item previous-page disable"><a class="page-link" href="#">Prev</a></li>
          <li class="page-item current-page active"><a class="page-link" href="#">1</a></li>
          <li class="page-item dots"><a class="page-link" href="#">...</a></li>
          <li class="page-item current-page"><a class="page-link" href="#">5</a></li>
          <li class="page-item current-page"><a class="page-link" href="#">6</a></li>
          <li class="page-item dots"><a class="page-link" href="#">...</a></li>
          <li class="page-item current-page"><a class="page-link" href="#">10</a></li>
          <li class="page-item next-page"><a class="page-link" href="#">Next</a></li>
        </div>
          
        </div>
        
      </div>
